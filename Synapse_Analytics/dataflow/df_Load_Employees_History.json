{
	"name": "df_Load_Employees_History",
	"properties": {
		"description": "This is a type 2 Mapping for loading Employees History",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "src_lenovo_employee",
						"type": "DatasetReference"
					},
					"name": "employees"
				},
				{
					"linkedService": {
						"referenceName": "synapse-prithwish-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "lkpEmployeesDelta"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "synapse-prithwish-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "insertDELTA"
				},
				{
					"name": "maxIDCache"
				},
				{
					"linkedService": {
						"referenceName": "synapse-prithwish-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "updateDELTA"
				},
				{
					"linkedService": {
						"referenceName": "synapse-prithwish-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "insertOLDrecords"
				}
			],
			"transformations": [
				{
					"name": "expKeyNonKey",
					"description": "create key and non key attributes"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "splitInsertUpdate"
				},
				{
					"name": "insertnew"
				},
				{
					"name": "select1"
				},
				{
					"name": "selDropLookupColumns",
					"description": "Dropping not required columns from lookup transformation"
				},
				{
					"name": "aggMAXID"
				},
				{
					"name": "selDropExceptEMPTABID"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1",
					"description": "creating the sequence generator"
				},
				{
					"name": "selDROP"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "insertOLD"
				},
				{
					"name": "select2"
				},
				{
					"name": "surrogateKey2"
				},
				{
					"name": "derivedColumn3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMP_ID as integer,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          GENDER as string,",
				"          DOB as date,",
				"          PHONE as string,",
				"          EMAIL as string,",
				"          PRESENT_ADR as string,",
				"          PERMANENT_ADR as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> employees",
				"source(output(",
				"          EMP_TABLE_ID as integer,",
				"          EMP_ID as string,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          GENDER as string,",
				"          DATE_OF_BIRTH as string,",
				"          PHONE as string,",
				"          EMAIL as string,",
				"          PRESENT_ADDRESS as string,",
				"          PERMANENT_ADDRESS as string,",
				"          Current_indicator as string,",
				"          effective_date as string,",
				"          last_date as string,",
				"          Key_attributes as string,",
				"          non_key_attributes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'delta-tables',",
				"     folderPath: 'Employees_Hist') ~> lkpEmployeesDelta",
				"employees derive(KEY_ATTRIBUTES = md5(EMP_ID,FIRST_NAME,LAST_NAME,GENDER),",
				"          NON_KEY_ATTRIBUTES = md5(PHONE,EMAIL,PRESENT_ADR,PERMANENT_ADR)) ~> expKeyNonKey",
				"expKeyNonKey, lkpEmployeesDelta lookup(expKeyNonKey@KEY_ATTRIBUTES == lkpEmployeesDelta@Key_attributes",
				"     && 'Y' == Current_indicator,",
				"     multiple: false,",
				"     pickup: 'first',",
				"     asc(EMP_TABLE_ID, true),",
				"     broadcast: 'right')~> lookup1",
				"selDropLookupColumns split(isNull(lkp_Key_attributes),",
				"     !isNull(lkp_Key_attributes) && (exp_NON_KEY_ATTRIBUTES!=lkp_non_key_attributes),",
				"     disjoint: false) ~> splitInsertUpdate@(INSERT, UPSERT, NOACTION)",
				"splitInsertUpdate@INSERT derive(Curr_ind = 'Y',",
				"          eff_date = currentTimestamp()) ~> insertnew",
				"insertnew select(mapColumn(",
				"          EMP_ID,",
				"          FIRST_NAME,",
				"          LAST_NAME,",
				"          GENDER,",
				"          DOB,",
				"          PHONE,",
				"          EMAIL,",
				"          PRESENT_ADR,",
				"          PERMANENT_ADR,",
				"          exp_KEY_ATTRIBUTES,",
				"          exp_NON_KEY_ATTRIBUTES,",
				"          Current_indicator,",
				"          Curr_ind,",
				"          eff_date",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"lookup1 select(mapColumn(",
				"          EMP_ID = employees@EMP_ID,",
				"          FIRST_NAME = employees@FIRST_NAME,",
				"          LAST_NAME = employees@LAST_NAME,",
				"          GENDER = employees@GENDER,",
				"          DOB,",
				"          PHONE = employees@PHONE,",
				"          EMAIL = employees@EMAIL,",
				"          PRESENT_ADR,",
				"          PERMANENT_ADR,",
				"          exp_KEY_ATTRIBUTES = expKeyNonKey@KEY_ATTRIBUTES,",
				"          exp_NON_KEY_ATTRIBUTES = expKeyNonKey@NON_KEY_ATTRIBUTES,",
				"          EMP_TABLE_ID,",
				"          Current_indicator,",
				"          lkp_Key_attributes = lkpEmployeesDelta@Key_attributes,",
				"          lkp_non_key_attributes = lkpEmployeesDelta@non_key_attributes",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selDropLookupColumns",
				"selDropExceptEMPTABID aggregate(EMP_TABLE_ID = max(EMP_TABLE_ID)) ~> aggMAXID",
				"lkpEmployeesDelta select(mapColumn(",
				"          EMP_TABLE_ID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selDropExceptEMPTABID",
				"select1 keyGenerate(output(row_number as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(Max_ID = coalesce(toInteger(maxIDCache#outputs()[1].EMP_TABLE_ID), 0) + toInteger(row_number)) ~> derivedColumn1",
				"splitInsertUpdate@UPSERT select(mapColumn(",
				"          EMP_ID,",
				"          Current_indicator",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selDROP",
				"selDROP alterRow(updateIf(true())) ~> alterRow1",
				"alterRow1 derive(Current_indicator = 'N') ~> derivedColumn2",
				"splitInsertUpdate@UPSERT derive(curr_ind = 'Y',",
				"          eff_dt = currentTimestamp()) ~> insertOLD",
				"insertOLD select(mapColumn(",
				"          EMP_ID,",
				"          FIRST_NAME,",
				"          LAST_NAME,",
				"          GENDER,",
				"          DOB,",
				"          PHONE,",
				"          EMAIL,",
				"          PRESENT_ADR,",
				"          PERMANENT_ADR,",
				"          exp_KEY_ATTRIBUTES,",
				"          exp_NON_KEY_ATTRIBUTES,",
				"          Current_indicator,",
				"          curr_ind,",
				"          eff_dt",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 keyGenerate(output(row_number as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey2",
				"surrogateKey2 derive(Max_ID = coalesce(toInteger(maxIDCache#outputs()[1].EMP_TABLE_ID), 0) + toInteger(row_number)) ~> derivedColumn3",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EMP_TABLE_ID as integer,",
				"          EMP_ID as string,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          GENDER as string,",
				"          DATE_OF_BIRTH as string,",
				"          PHONE as string,",
				"          EMAIL as string,",
				"          PRESENT_ADDRESS as string,",
				"          PERMANENT_ADDRESS as string,",
				"          Current_indicator as string,",
				"          effective_date as string,",
				"          last_date as string,",
				"          Key_attributes as string,",
				"          non_key_attributes as string",
				"     ),",
				"     format: 'delta',",
				"     fileSystem: 'delta-tables',",
				"     folderPath: 'Employees_Hist',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 2,",
				"     mapColumn(",
				"          EMP_TABLE_ID = Max_ID,",
				"          EMP_ID,",
				"          FIRST_NAME,",
				"          LAST_NAME,",
				"          GENDER,",
				"          DATE_OF_BIRTH = DOB,",
				"          PHONE,",
				"          EMAIL,",
				"          PRESENT_ADDRESS = PRESENT_ADR,",
				"          PERMANENT_ADDRESS = PERMANENT_ADR,",
				"          Current_indicator,",
				"          effective_date = eff_date,",
				"          Key_attributes = exp_KEY_ATTRIBUTES,",
				"          non_key_attributes = exp_NON_KEY_ATTRIBUTES",
				"     )) ~> insertDELTA",
				"aggMAXID sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1) ~> maxIDCache",
				"derivedColumn2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'delta-tables',",
				"     folderPath: 'Employees_Hist',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: true,",
				"     upsertable: false,",
				"     keys:['EMP_ID'],",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 2,",
				"     mapColumn(",
				"          EMP_ID,",
				"          Current_indicator",
				"     )) ~> updateDELTA",
				"derivedColumn3 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EMP_TABLE_ID as integer,",
				"          EMP_ID as string,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          GENDER as string,",
				"          DATE_OF_BIRTH as string,",
				"          PHONE as string,",
				"          EMAIL as string,",
				"          PRESENT_ADDRESS as string,",
				"          PERMANENT_ADDRESS as string,",
				"          Current_indicator as string,",
				"          effective_date as string,",
				"          last_date as string,",
				"          Key_attributes as string,",
				"          non_key_attributes as string",
				"     ),",
				"     format: 'delta',",
				"     fileSystem: 'delta-tables',",
				"     folderPath: 'Employees_Hist',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 2,",
				"     mapColumn(",
				"          EMP_TABLE_ID = Max_ID,",
				"          EMP_ID,",
				"          FIRST_NAME,",
				"          LAST_NAME,",
				"          GENDER,",
				"          DATE_OF_BIRTH = DOB,",
				"          PHONE,",
				"          EMAIL,",
				"          PRESENT_ADDRESS = PRESENT_ADR,",
				"          PERMANENT_ADDRESS = PERMANENT_ADR,",
				"          Key_attributes = exp_KEY_ATTRIBUTES,",
				"          non_key_attributes = exp_NON_KEY_ATTRIBUTES,",
				"          Current_indicator,",
				"          Current_indicator = curr_ind,",
				"          effective_date = eff_dt",
				"     )) ~> insertOLDrecords"
			]
		}
	}
}