{
	"name": "ds_RawFixedWidthFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "synapse-prithwish-WorkspaceDefaultStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"p_FileSystem": {
				"type": "string",
				"defaultValue": "daily-feed-load"
			},
			"p_Directory": {
				"type": "string",
				"defaultValue": "landing"
			},
			"p_FileName": {
				"type": "string",
				"defaultValue": "LIFE_POLICY_DAILY.txt"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().p_FileSystem",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().p_FileName",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().p_Directory",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	}
}